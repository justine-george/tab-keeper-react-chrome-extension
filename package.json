{
  "name": "tab-keeper-react-chrome-extension",
  "private": true,
  "version": "1.1.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-pretty": "tsc && npx prettier -w . && vite build",
    "sync-versions": "node package_to_manifest.cjs",
    "bump-version": "npm version patch --no-git-tag-version && npm run sync-versions",
    "release-local": "tsc && npx prettier -w . && vite build && cd dist && node ../zip.cjs",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.5",
    "firebase": "^10.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "redux-thunk": "^2.4.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.243",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^3.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  }
}
